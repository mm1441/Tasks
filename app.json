{
  "expo": {
    "name": "tasks",
    "slug": "tasks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/vecteezy_checklist.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "com.magicmarinac.tasks",
    "ios": {
      "bundleIdentifier": "com.magicmarinac.tasks",
      "supportsTablet": true,
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.magicmarinac.tasks.expowidgets"
        ],
        "aps-environment": "development"
      },
      "infoPlist": {
        "NSSupportsLiveActivities": false,
        "NSSupportsLiveActivitiesFrequentUpdates": false,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.magicmarinac.tasks",
      "adaptiveIcon": {
        "foregroundImage": "assets/vecteezy_checklist.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-font",
      "expo-web-browser",
      [
        "@bittingz/expo-widgets",
        {
          "ios": {
            "src": "./widgets/ios",
            "devTeamId": "YOUR_APPLE_DEV_TEAM_ID",
            "mode": "development",
            "useLiveActivities": false,
            "frequentUpdates": false,
            "entitlements": {
              "com.apple.security.application-groups": [
                "com.magicmarinac.tasks.widgets"
              ]
            }
          },
          "android": {
            "src": "./widgets/android",
            "distPlaceholder": "package_name",
            "widgets": [
              {
                "name": "TasksWidgetProvider",
                "resourceName": "@xml/tasks_widget_info"
              }
            ]
          }
        }
      ],
      "./plugins/withTasksWidgetSetup"
    ],
    "splash": {
      "image": "assets/vecteezy_checklist.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "extra": {
      "eas": {
        "projectId": "e5d392f8-451d-40ba-b9de-6ac26f027771"
      }
    }
  }
}
